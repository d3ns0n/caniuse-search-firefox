{
  "manifest_version": 2,
  "name": "Can I use search engine",
  "description": "Adds a search engine that searches https://caniuse.com",
  "version": "0.1",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Can I use",
      "search_url": "https://caniuse.com/#search={searchTerms}",
      "keyword": "ciu",
      "favicon_url": "https://caniuse.com/img/favicon-128.png",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
